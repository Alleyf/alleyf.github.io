{
  "defaultViewMode": "source",
  "spellcheckLanguages": [
    "zh-CN",
    "en-US"
  ],
  "userIgnoreFilters": [
    "node_modules/",
    "public/",
    "scaffolds/",
    "scripts/",
    "themes/"
  ],
  "strictLineBreaks": true,
  "showLineNumber": true,
  "showFrontmatter": true,
  "vimMode": false,
  "livePreview": true,
  "promptDelete": false,
  "pdfExportSettings": {
    "includeName": true,
    "pageSize": "Letter",
    "landscape": false,
    "margin": "0",
    "downscalePercent": 100
  }
}